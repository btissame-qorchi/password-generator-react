{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/password-generator-react/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "headers": [
    {
      "source": "/dist/assets/*.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    }
  ]
}
